{
  "expo": {
    "name": "Studify",
    "slug": "studify",
    "version": "1.2.0",
    "orientation": "portrait",
    "icon": "./assets/studify.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/studify.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a1a2e"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.studify.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/studify.png",
        "backgroundColor": "#1a1a2e"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.studify.app",
      "versionCode": 1,
      "permissions": [
        "VIBRATE",
        "POST_NOTIFICATIONS"
      ]
    },
    "web": {
      "favicon": "./assets/studify.png"
    },
    "plugins": [
      "expo-font",
      "@react-native-community/datetimepicker",
      [
        "expo-notifications",
        {
          "icon": "./assets/studify.png",
          "color": "#e94560",
          "sounds": []
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "f466dd7b-dd33-4c10-9571-219700dce03c"
      }
    }
  }
}
